    <Duoxuan @func=\"getMessage\"></Duoxuan>-->\r\n    <!--    <Tiankong @func=\"getMessage\"></Tiankong>-->\r\n    <!--    <Pingfen @func=\"getMessage\"></Pingfen>-->\r\n    <!--    -->\r\n\r\n    <br>\r\n    <!--    {{ question }}-->\r\n    <ul>\r\n      <li v-for=\"item in question\" style=\"list-style: none\">\r\n        <i class=\"el-icon-edit\"></i><strong> 第 </strong><strong\r\n          style=\"color: deepskyblue\">{{ item.bianhao }}</strong><strong> 题</strong>\r\n        <Danxuan v-if=\"item.level === '1'\" @func=\"getMessage\" :tigan=\"1\" :bianhao=\"1\"\r\n                 :xuanxiang=\"item.xuanxiang.split(';')\" :id=\"item.id - 1\"></Danxuan>\r\n        <Duoxuan v-else-if=\"item.level === '2'\" @func=\"getMessage\" :tigan=\"item.context\" :bianhao=\"item.bianhao\"\r\n                 :xuanxiang=\"item.xuanxiang.split(';')\" :id=\"item.id - 1\"></Duoxuan>\r\n        <Tiankong v-else-if=\"item.level === '3'\" @func=\"getMessage\" :tigan=\"item.context\"\r\n                  :bianhao=\"item.bianhao\" :id=\"item.id - 1\"></Tiankong>\r\n        <Pingfen v-else-if=\"item.level === '4'\" @func=\"getMessage\" :tigan=\"item.context\" :bianhao=\"item.bianhao\"\r\n                 :id=\"item.id - 1\"></Pingfen>\r\n        <br>\r\n      </li>\r\n    </ul>\r\n<!--    <el-button @click=\"test\">test</el-button>-->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Danxuan from \"@/components/Danxuan\";\r\nimport Duoxuan from \"@/components/Duoxuan\";\r\nimport Tiankong from \"@/components/Tiankong\";\r\nimport Pingfen from \"@/components/Pingfen\"\r\n\r\nexport default {\r\n  name: \"TrueWenJuan\",\r\n  components: {\r\n    Danxuan,\r\n    Duoxuan,\r\n    Tiankong,\r\n    Pingfen\r\n  },\r\n  mounted() {\r\n    this.axios({\r\n      method: 'POST',\r\n      url: 'http://127.0.0.1:8000/consumer/getallQ',\r\n      // url: 'http://42.193.111.182:8000/consumer/getallQ',\r\n      data: {\r\n        id: this.$route.query.Qid\r\n      }\r\n    }).then(response => {\r\n      console.log(response)\r\n      // setTimeout(this.question = response.data.question, 1000);\r\n      this.question = response.data.question\r\n    })\r\n  },\r\n  data() {\r\n    return {\r\n      wenjuanname: '从后端返回问卷名',\r\n      name: '从后端返回用户名',\r\n      answer: [],\r\n      question: [\r\n        {\r\n          level: 2,\r\n          context: '这里是第一题：多选题',\r\n          xuanxiang: ['A', 'B', 'C'],\r\n          bianhao: 1\r\n        },\r\n        {\r\n          level: 3,\r\n          context: '这里是第二题：填空题',\r\n          xuanxiang: [],\r\n          bianhao: 2\r\n        },\r\n        {\r\n          level: 1,\r\n          context: '这里是第三题：单选题',\r\n          xuanxiang: ['A', 'B'],\r\n          bianhao: 3\r\n        },\r\n        {\r\n          level: 4,\r\n          context: '这里是第四题：评分题',\r\n          xuanxiang: [],  //没有选项的题目选项传入空对象\r\n          bianhao: 4  //编号可以理解为题号\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    test() {\r\n      console.log(this.question[0].xuanxiang === \"1;1\")\r\n    },\r\n    getMessage(data) {\r\n      let count = false\r\n      // console.log(\"父组件接收到了子组件传来的信息：\", data)\r\n      for (let i = 0; i <= this.answer.length - 1; i++) {\r\n        if (this.answer[i].bianhao === data.bianhao) {\r\n          // console.log(\"您改变了原来的答案，所以这里数组不能采用push方法\")\r\n          this.answer.splice(i, 1, data)\r\n          console.log(\"answer[] = \", this.answer)\r\n          count = true\r\n        }\r\n      }\r\n      if (count === false) {\r\n        this.answer.push(data)\r\n        console.log(\"answer=\", this.answer)\r\n      }\r\n    },\r\n    sendMessage() {\r\n      console.log(\"您当前的答题情况为\", this.answer)\r\n      this.axios({\r\n        method: 'POST',\r\n        url: 'http://127.0.0.1:8000/consumer/huishoudaan',\r\n        // url: 'http://42.193.111.182:8000/consumer/huishoudaan',\r\n        data: {\r\n          id: this.$route.query.Qid,\r\n          question: this.answer\r\n        }\r\n      }).then(response => {\r\n        console.log(response)\r\n        alert(\"答案已经提交\")\r\n      })\r\n      // this.$router.push({path: '/person', query: {account: this.account}})\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.true-div {\r\n  margin-top: 100px;\r\n  margin-left: 150px;\r\n  margin-right: 150px;\r\n}\r\n</style>\r\n"]}]}