data=\"tableData.filter(data => !search || data.name.toLowerCase().includes(search.toLowerCase()))\"\r\n        style=\"width: 100%\">\r\n      <el-table-column\r\n          label=\"问卷ID\"\r\n          prop=\"id\">\r\n      </el-table-column>\r\n      <el-table-column\r\n          label=\"问卷标题\"\r\n          prop=\"title\">\r\n      </el-table-column>\r\n<!--      <el-table-column-->\r\n<!--          label=\"发布时间\"-->\r\n<!--          prop=\"end\">-->\r\n<!--      </el-table-column>-->\r\n<!--      <el-table-column-->\r\n<!--          label=\"结束时间\"-->\r\n<!--          prop=\"start\">-->\r\n<!--      </el-table-column>-->\r\n      <el-table-column\r\n          label=\"创建日期\"\r\n          prop=\"title\">\r\n      </el-table-column>\r\n      <el-table-column\r\n          label=\"问卷状态\"\r\n          prop=\"title\">\r\n      </el-table-column>\r\n      <el-table-column\r\n          align=\"right\">\r\n        <template slot=\"header\" slot-scope=\"scope\">\r\n          <el-input\r\n              v-model=\"search\"\r\n              size=\"mini\"\r\n              placeholder=\"输入关键字搜索\"\r\n              class=\"test1\"\r\n              style=\"width: 200px\"\r\n          />\r\n        </template>\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n              size=\"mini\"\r\n              type=\"primary\"\r\n              @click=\"shujufenxi(scope.$index, scope.row)\"\r\n              icon=\"el-icon-pie-chart\">数据分析\r\n          </el-button>\r\n          <el-button\r\n              size=\"mini\"\r\n              @click=\"handleEdit(scope.$index, scope.row)\"\r\n              icon=\"el-icon-edit-outline\">编辑问卷\r\n          </el-button>\r\n          <br><br>\r\n          <el-button\r\n              size=\"mini\"\r\n              type=\"danger\"\r\n              @click=\"handleDelete(scope.$index, scope.row)\"\r\n          icon=\"el-icon-folder-delete\">删除问卷\r\n          </el-button>\r\n          &nbsp\r\n          <el-dropdown>\r\n            <el-button type=\"primary\" size=\"mini\">\r\n              更多菜单<i class=\"el-icon-arrow-down el-icon--right\"></i>\r\n            </el-button>\r\n            <el-dropdown-menu slot=\"dropdown\">\r\n              <span @click=\"clickmethod3(scope.$index, scope.row)\"><el-dropdown-item>浏览问卷</el-dropdown-item></span>\r\n              <span @click=\"clickmethod4\"><el-dropdown-item>发布问卷</el-dropdown-item></span>\r\n            </el-dropdown-menu>\r\n          </el-dropdown>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Wenjuan',\r\n  mounted() {\r\n    this.search1()\r\n  },\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      search: ''\r\n    }\r\n  },\r\n  methods: {\r\n    clickmethod3(index, row) {\r\n      console.log(\"点击浏览问卷\",index,row.id)\r\n      // this.$router.replace('/view')\r\n      this.$router.push({path: '/view', query: {account: this.$route.query.id,id: row.id}})\r\n\r\n    },\r\n    clickmethod4() {\r\n      console.log(\"点击发布问卷\")\r\n    },\r\n    handleEdit(index, row) {\r\n      console.log(index, row);\r\n      this.axios({\r\n        method: 'POST',\r\n        url: 'http://127.0.0.1:8000/consumer/getallQ',\r\n        // url: 'http://42.193.111.182:8000/consumer/getallQ',\r\n        data: {\r\n          id: 17\r\n        }\r\n      }).then(response => {\r\n        console.log(response)\r\n      })\r\n      this.$router.push({path: '/edit', query: {account: this.$route.query.id,id: row.id}})\r\n\r\n    },\r\n    handleDelete(index, row) {\r\n      console.log(index, row);\r\n      this.tableData.splice(index, 1)\r\n      this.axios({\r\n        method: 'POST',\r\n        url: 'http://127.0.0.1:8000/consumer/e_qustionnare',\r\n        // url: 'http://42.193.111.182:8000/consumer/e_qustionnare',\r\n        data: {\r\n          id: row.id\r\n        }\r\n      }).then(response => {\r\n        console.log(response)\r\n      })\r\n    },\r\n    add() {\r\n\r\n      console.log(\"您点击了新建问卷方法\")\r\n      console.log(this.tableData)\r\n      // this.tableData.push(\r\n      //     {\r\n      //       date: '2016-05-03',\r\n      //       name: '王小虎',\r\n      //       address: '上海市普陀区金沙江路 1516 弄'\r\n      //     })\r\n      this.$router.push({path: '/new1', query: {id: this.$route.query.id}})\r\n\r\n    },\r\n    search1() {\r\n      console.log(\"点击了查找用户所有问卷方法\")\r\n      this.axios({\r\n        method: 'POST',\r\n        url: 'http://127.0.0.1:8000/consumer/getQN',\r\n        // url: 'http://42.193.111.182:8000/consumer/getQN',\r\n        data: {\r\n          account: this.$route.query.id\r\n        }\r\n      }).then(response => {\r\n        console.log(response.data)\r\n        this.tableData = []\r\n        let i = 0\r\n        for (i = 0; i <= response.data['question'].length - 1; i++) {\r\n          if (response.data['question'][i].end === null) {\r\n            response.data['question'][i].end = \"暂无定义\"\r\n          }\r\n          if (response.data['question'][i].start === null) {\r\n            response.data['question'][i].start = \"暂无定义\"\r\n          }\r\n          this.tableData.push(response.data['question'][i])\r\n        }\r\n        console.log(\"tableData\", this.tableData)\r\n      })\r\n    },\r\n    shujufenxi($index, row) {\r\n      console.log(\"您点击了数据分析功能\")\r\n      this.axios({\r\n        method: 'POST',\r\n        url: 'http://127.0.0.1:8000/consumer/shujufenxi',\r\n        // url: 'http://42.193.111.182:8000/consumer/shujufenxi',\r\n        data: {\r\n          id: row.id,\r\n        }\r\n      }).then(response => {\r\n        console.log(response)\r\n      })\r\n      // this.$router.replace('/shujufenxi')\r\n      this.$router.push({path: '/shujufenxi', query: {id: row.id}})\r\n    }\r\n\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"]}]}