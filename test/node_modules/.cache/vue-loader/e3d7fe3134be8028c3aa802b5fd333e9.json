   width=\"180\">\r\n          <template slot-scope=\"scope\">\r\n            <i class=\"el-icon-time\"></i>\r\n            <span style=\"margin-left: 10px\">{{ scope.row.date }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n            label=\"题目数量\"\r\n            width=\"180\">\r\n          <template slot-scope=\"scope\">\r\n            <el-popover trigger=\"hover\" placement=\"top\">\r\n              <p>问卷名: {{ scope.row.name }}</p>\r\n              <p>问卷id: {{ scope.row.address }}</p>\r\n              <div slot=\"reference\" class=\"name-wrapper\">\r\n                <el-tag size=\"medium\">{{ scope.row.name }}</el-tag>\r\n              </div>\r\n            </el-popover>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n                size=\"mini\"\r\n                @click=\"handleEdit(scope.$index, scope.row)\">回答问卷</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n\r\n\r\n  </div>\r\n\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Answer\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      wenjuanid: ''\r\n    }\r\n  },\r\n  methods: {\r\n    searchMethod() {\r\n      this.axios({\r\n        method: 'POST',\r\n        url: 'http://127.0.0.1:8000/consumer/getallQ',\r\n        // url: 'http://42.193.111.182:8000/consumer/getallQ',\r\n        data: {\r\n          id: this.wenjuanid\r\n        }\r\n      }).then(response => {\r\n        console.log(response)\r\n        if (response.data.question === []){\r\n          alert(\"没有查询到对应问卷或对应问卷为空\")\r\n        }else {\r\n          this.tableData.push(\r\n              {\r\n                date: this.wenjuanid,\r\n                name: response.data.question.length,\r\n              }\r\n          )\r\n        }\r\n      })\r\n    },\r\n    handleEdit(index, row) {\r\n      console.log(index, row);\r\n      console.log(\"您点击了查看问卷方法\")\r\n      this.$router.push({path: '/wenjuan', query: {account: this.$route.query.id,Qid: this.wenjuanid}})\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.answer-div {\r\n  background-color: white;\r\n  width: 1000px;\r\n  height: 500px;\r\n  box-shadow: 0 2px 4px rgba(0, 0, 0, .12), 0 0 6px rgba(0, 0, 0, .04);\r\n  border-radius: 4px\r\n}\r\n\r\n.form-div {\r\n  margin: 50px;\r\n}\r\n</style>\r\n"]}]}